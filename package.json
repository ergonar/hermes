    "@shelf/jest-mongodb": "^1.2.3",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.12",
    "@types/mongoose": "^5.7.36",
    "faker": "^5.1.0",
    "jest": "^26.4.2",
    "node-mocks-http": "^1.9.0",
    "set-cookie-parser": "^2.4.6",
    "superagent": "^6.1.0",
    "supertest": "^4.0.2",
